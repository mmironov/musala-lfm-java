package com.musala.lfm;

public class Program {

	public static void main(String[] args) {
		Account account = new Account("lots of money");
		account.deposit(1000);
		account.deposit(25);
		
		account.lock();
		
		account.withdraw(1000000);
		
		account.open();
		
		System.out.println(account);
	}
}
