		for(int i=0; i < n; ++i) {
			
			for(int j=0; j < i; ++j) {
				
			}
		}
	}
	
	int binarySearch(int[] numbers, int x) {
		
		int begin = 0;
		int end = numbers.length - 1;
		int mid = (begin + end) / 2;
		
		while (begin <= end) {
			
			if (numbers[mid] == x) {
				return mid;
			}
			
			if (x < numbers[mid]) {
				end = mid - 1;
			}
		}
	}
	
	public static void main(String[] args) {
		
	}
}
