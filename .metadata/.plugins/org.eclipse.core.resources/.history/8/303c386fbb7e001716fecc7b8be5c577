package com.musala.lfm;

public class StringsExample {

	public static void main(String[] args) {
		
		String hello = "hello";
		String world = "world";
		
		String helloWorld = hello + ", " + world;
		
		helloWorld += "!";
		
		String str = helloWorld;
		
		System.out.println(helloWorld);
		
		System.out.println("Length: " + str.length());
	}
}
