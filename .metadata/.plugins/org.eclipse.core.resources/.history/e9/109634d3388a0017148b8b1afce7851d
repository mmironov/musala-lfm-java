package com.musala.lfm;

import java.net.URL;

import javax.swing.Icon;
import javax.swing.ImageIcon;
import javax.swing.JOptionPane;

public class Program {

	public static void main(String[] args) {
		
		Icon icon = new ImageIcon(new URL("https://docs.gimp.org/en/images/filters/examples/color-taj-sample-colorize.jpg"), "India");
		
		JOptionPane.showMessageDialog(null, "Hello world!", "Info",JOptionPane.WARNING_MESSAGE, icon);
	}
}
