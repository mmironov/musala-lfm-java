package com.musala.lfm;

public class Account {

	private String name;
	private double balance;
	private boolean isOpen;
	
	public Account() {
		this("");
	}
	
	public Account(String name) {
		this.name = name;
		balance = 0;
		isOpen = false;
	}
	
	public Account(String name, double balance, boolean isOpen) {
		this.name = name;
		this.balance = balance;
		this.isOpen = isOpen;
	}
}
