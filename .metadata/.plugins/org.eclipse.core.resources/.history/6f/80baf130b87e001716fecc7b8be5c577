package com.musala.lfm;

public class ArraysExample {

	public static void main(String[] args) {
		
		int[] array = new int[5];
		array[0] = 10;
		array[1] = 20;
		
		int[] initializedArray = {1, 2, 3};
	}
	
	static void printArray(int[] array) {
		
		for(int i=0; i < array.length; ++i) {
			System.out.print(array[i] + " ")''
		}
	}
}
